#!/bin/bash
if [ -n "$1" ]; then
  for file in "$@"; do
    echo 'Uploading $file...' >&2
    curl --user ':vKIz/IFRYtZi6CwAB7Ui+clT3lt50sSTpwSUAjR1hyQ=' \
         -X POST -H "Content-Type: application/json" -d @$file \
         'https://davar.icfpcontest.org/teams/45/solutions'
  done
else
	echo "Usage: $0 <jsonfile>"
fi
